<EmberWormhole @to="{{@containerId}}-outlet-menu" @renderInPlace={{this.renderInPlace}}>
  <div class='bm-menu' style={{this.style}}>
    {{yield
      (hash
        item=(component
          'bm-menu-item'
          tagName=this.itemTagName
          menuItems=this.menuItems
          dismissOnClick=this.dismissOnItemClick
          state=this.state
        )
      )
    }}
  </div>
</EmberWormhole>